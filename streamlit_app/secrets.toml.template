# Streamlit Secrets Configuration for Snowflake Connection
# Save this as ~/.streamlit/secrets.toml or in the Streamlit Cloud secrets manager

[snowflake]
account = "your_account_id"
user = "your_username"
password = "your_password"
role = "TRAINING_APP_ROLE"
warehouse = "TRAINING_WH"
database = "TRAINING_DB"
schema = "PUBLIC"
client_session_keep_alive = true
